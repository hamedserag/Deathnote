@model FBClone.Models.User

<h2>ChangeImage</h2>

<form action="ChangeImage" method="post" enctype="multipart/form-data">
    <div class="row">
        <input type="file" name="img" value="file" />
        <button class="col col-10 login_btn" type="submit">Change Photo</button>

    </div>
    @ViewBag.error
</form>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
