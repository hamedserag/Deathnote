@model FBClone.Models.User

<h2 class="text-center">ChangeImage</h2>

<form action="ChangeImage" method="post" enctype="multipart/form-data">
    <div class="row justify-content-center">
        <input class="col-2" type="file" name="img" value="file" />
        <button class="buttonDefault btn col-2 login_btn" type="submit">Change Photo</button>

    </div>
    @ViewBag.error
</form>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
